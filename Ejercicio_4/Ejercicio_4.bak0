           textmsg("Pulido en fila "+to_str(j)+" columna "+to_str(i)+" finalizado")
           varx=varx+d_filas
           j=j+1
          end
         varx=0
         j=1
         vary=vary+d_columnas
         i=i+1
        end
        set_digital_out(0,False)
        end
  
